<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<%
String path=request.getContextPath();
String basePath=request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>
<!DOCTYPE>
<html>
<head>
<base href="<%=basePath%>">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>2级企业注册</title>
<script src="js/index/jquery-3.4.1.min.js"></script>
<script src="js/index/resetPassword.js"></script>
</head>
<body>
		<div style="margin-top:100px;" >
			<form>
				<table align="center" border="0" cellspacing="10" >
					<tr>
						<td>原密码：</td>
						<td><input type="password" name="oldPass" id="o_pwd"/><font color="red"></font> </td>
					</tr>
					<tr>
						<td>新密码：</td>
						<td><input type="password" name="corporate_password" id="n_pwd1" /><font color="red"></font> </td>
					</tr>
					<tr>
						<td>确认密码：</td>
						<td><input type="password" id="n_pwd2"/><font color="red"></font> </td>
					</tr>
					<tr>
						<td colspan="2" style="text-align: right;">
							<input  type="button" id="ok" value="提交"/>
						</td>
					</tr>
				</table>
			</form>
		</div>
	</body>
</html>
